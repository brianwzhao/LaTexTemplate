fun taut ([]:bool list) = true | taut(a::b) = a andalso taut(b);

fun isValid(a:((bool list)->bool) list)(b:(bool list)->bool)(c:bool list) =
  if (not(b c)) then not (taut (map (fn d:(bool list)->bool => d c) a)) else true;
