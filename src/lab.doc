fun q(a:bool list) = List.nth(a,1);
fun r(a:bool list) = List.nth(a,2);
fun s(a:bool list) = List.nth(a,3);
fun t(a:bool list) = List.nth(a,4);
val title = "Test Cases Slide A:b";
fun s1(a) = ! (p a) && (q a);
fun s2(a) = (r a)|| (p a);
isValid [s1,s2] r;
isValid [s1,s2] p;
