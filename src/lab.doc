(* Nested parameter builder *)
fun nas(a:'a,b:'a)(n:int):'a list list =
  let
    fun an(first,second,input) =
      ((map (fn a=> first::a) input)
      @ (map(fn a=>second::a)input))
  in
  if n = 1 then
    [[a],[b]]
  else
    an (a, b, (nas(a,b)(n-1)))
  end;
val na = nas(1,0);
=TEX
