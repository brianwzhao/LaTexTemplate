{
=SML
fun inOrder(x) = case x of elems [] => true
 | elems(x::xs::xss)=> greaterElem(xs,x)
    andalso (inOrder(elems(xs::xss)))
 | elems(x::[])=>true;

fun sort(elems(x::xs)) = (case x of
  Set x =>merge(elems[Set(sort x)],sort(elems xs))
 | _ => merge(elems [x],sort(elems xs)))
 | sort(elems[]) = elems [];
