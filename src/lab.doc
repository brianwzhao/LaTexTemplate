  not (greaterElem(x,y) orelse greaterElem(y,x));

fun greaterSet(elems(x::xs),elems(y::ys)) =
  if isEqual (x,y)
    then greaterSet(elems xs,elems ys)
    else greaterElem(x,y)
 | greaterSet(elems[],_) = false
 | greaterSet(_,elems []) = true;
