fun ||(0,q)=q | ||(1,_)=1
 |  ||(_,_)=2;
fun !(0)=1 | !(1)=0
 |  !(_)=2;
fun xor(0,q)=q | xor(1,q)= !(q)
 |  xor(_,_)= 2;
fun uh(arr:int*int->int)(a:int list):int
  = arr (List.nth(a,0), List.nth(a,1));
map (uh(||))(na(2));
map (uh(&&))(na(2));
map (uh(xor))(na(2));
infix &&;infix ||;infix xor; 
